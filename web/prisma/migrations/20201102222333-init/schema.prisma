datasource db {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Offer {
  id Int @id @default(autoincrement())
  name String
  urlImage String
  urlOffer String
  descroption String
  offerPrice Float
  normalPrice Float
  offerText String
  store String
}
